<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:import namespace="http://www.w3.org/XML/1998/namespace"/>
	<xs:element name="amendment-doc">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pre-form" minOccurs="0"/>
				<xs:choice>
					<xs:sequence>
						<xs:element ref="amendment-form"/>
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:element ref="amendment-body"/>
							<xs:element ref="preamble-amends"/>
						</xs:choice>
					</xs:sequence>
					<xs:sequence>
						<xs:element ref="conference-report-form"/>
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:element ref="amendment-body"/>
							<xs:element ref="preamble-amends"/>
						</xs:choice>
					</xs:sequence>
					<xs:sequence>
						<xs:element ref="engrossed-amendment-form"/>
						<xs:element ref="engrossed-amendment-body"/>
						<xs:element ref="preamble-amends" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:choice>
				<xs:element ref="title-amends" minOccurs="0"/>
				<xs:element ref="attestation" minOccurs="0"/>
				<xs:element ref="endorsement" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="amend-type">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="motion"/>
						<xs:enumeration value="engrossed-amendment"/>
						<xs:enumeration value="conference-report"/>
						<xs:enumeration value="insert"/>
						<xs:enumeration value="senate-amendment"/>
						<xs:enumeration value="house-amendment"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="amend-stage">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="pending"/>
						<xs:enumeration value="submitted"/>
						<xs:enumeration value="proposed"/>
						<xs:enumeration value="approved"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="amend-degree">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="sixth"/>
						<xs:enumeration value="third"/>
						<xs:enumeration value="second"/>
						<xs:enumeration value="first"/>
						<xs:enumeration value="fifth"/>
						<xs:enumeration value="fourth"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="dms-id" type="xs:ID"/>
			<xs:attribute name="dms-version" type="xs:anySimpleType"/>
			<xs:attribute name="style" default="OLC">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="multiple-resolved-clause"/>
						<xs:enumeration value="USC"/>
						<xs:enumeration value="OLC"/>
						<xs:enumeration value="tax"/>
						<xs:enumeration value="impeachment"/>
						<xs:enumeration value="appropriations"/>
						<xs:enumeration value="concur-with-amendment"/>
						<xs:enumeration value="defense"/>
						<xs:enumeration value="traditional-inline"/>
						<xs:enumeration value="constitutional-amendment"/>
						<xs:enumeration value="traditional"/>
						<xs:enumeration value="order-of-business"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="star-print" default="no-star-print">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="no-star-print"/>
						<xs:enumeration value="second-star-print"/>
						<xs:enumeration value="fourth-star-print"/>
						<xs:enumeration value="first-star-print"/>
						<xs:enumeration value="third-star-print"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="public-print" default="no">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="no"/>
						<xs:enumeration value="yes"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="marker">
		<xs:complexType/>
	</xs:element>
	<xs:element name="meta-house">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="holc-filename" minOccurs="0"/>
				<xs:element ref="holc-attorney" minOccurs="0"/>
				<xs:element ref="holc-last-author" minOccurs="0"/>
				<xs:element ref="holc-last-saved" minOccurs="0"/>
				<xs:element ref="holc-creator" minOccurs="0"/>
				<xs:element ref="holc-creation-date" minOccurs="0"/>
				<xs:element ref="holc-contact" minOccurs="0"/>
				<xs:element ref="holc-contact-phone" minOccurs="0"/>
				<xs:element ref="conversion-notes" minOccurs="0"/>
				<xs:element ref="version" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="holc-job-number" minOccurs="0"/>
				<xs:element ref="holc-doc-number" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="holc-filename">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="marker"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="holc-attorney">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="marker"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="holc-job-number">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="marker"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="holc-doc-number">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="marker"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="holc-last-author">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="marker"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="holc-last-saved">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="marker"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="holc-creator">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="marker"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="holc-creation-date">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="marker"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="holc-contact">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="marker"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="holc-contact-phone">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="marker"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="conversion-notes">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="noted" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="noted">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="marker"/>
			</xs:choice>
			<xs:attribute name="idref" type="xs:IDREF"/>
			<xs:attribute name="error-level" default="5">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="1"/>
						<xs:enumeration value="2"/>
						<xs:enumeration value="3"/>
						<xs:enumeration value="4"/>
						<xs:enumeration value="5"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="version">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="version-filename"/>
				<xs:element ref="version-date"/>
				<xs:element ref="version-creator"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="version-filename">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="marker"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="version-date">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="marker"/>
