			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="meta-senate">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="slc-for" minOccurs="0"/>
				<xs:element ref="slc-subject" minOccurs="0"/>
				<xs:element ref="slc-type" minOccurs="0"/>
				<xs:element ref="slc-description" minOccurs="0"/>
				<xs:element ref="slc-received" minOccurs="0"/>
				<xs:element ref="slc-delivered" minOccurs="0"/>
				<xs:element ref="slc-staff" minOccurs="0"/>
				<xs:element ref="slc-peer-reviewed-by" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="slc-for-office">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="slc-for-sponsor">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="slc-for-staffer">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="slc-for">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="slc-for-office" minOccurs="0"/>
				<xs:element ref="slc-for-sponsor" minOccurs="0"/>
				<xs:element ref="slc-for-staffer" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="slc-subject-field1">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="slc-subject-field2">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="slc-subject-field3">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="slc-subject">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="slc-subject-field1" minOccurs="0"/>
				<xs:element ref="slc-subject-field2" minOccurs="0"/>
				<xs:element ref="slc-subject-field3" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="slc-type">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="slc-description">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="added-phrase"/>
				<xs:element ref="deleted-phrase"/>
				<xs:element ref="internal-xref"/>
				<xs:element ref="external-xref"/>
				<xs:element ref="fraction"/>
				<xs:element ref="superscript"/>
				<xs:element ref="subscript"/>
				<xs:element ref="italic"/>
				<xs:element ref="quote"/>
				<xs:element ref="inline-comment"/>
				<xs:element ref="act-name"/>
				<xs:element ref="formula"/>
				<xs:element ref="term"/>
				<xs:element ref="omitted-text"/>
				<xs:element ref="linebreak"/>
				<xs:element ref="nobreak"/>
				<xs:element ref="pagebreak"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="slc-received">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="slc-delivered">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="slc-staff">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="slc-peer-reviewed-by">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="pre-form">
		<xs:complexType>
			<xs:sequence>
				<xs:choice minOccurs="0">
					<xs:element ref="meta-house"/>
					<xs:element ref="meta-senate"/>
				</xs:choice>
				<xs:element ref="author-note" minOccurs="0"/>
				<xs:element ref="running-header" minOccurs="0"/>
				<xs:element ref="suspension-floor-action" minOccurs="0"/>
				<xs:element ref="suspension-instructions" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="legis-counsel" minOccurs="0"/>
				<xs:element ref="reintroduction-code" minOccurs="0"/>
				<xs:element ref="signature-line" minOccurs="0"/>
				<xs:element ref="first-page-header" minOccurs="0"/>
				<xs:element ref="first-page-date" minOccurs="0"/>
				<xs:element ref="first-page-desc" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="display" default="yes">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="no"/>
						<xs:enumeration value="yes"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="author-note">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="marker"/>
			</xs:choice>
			<xs:attribute name="display" default="yes">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="no"/>
						<xs:enumeration value="yes"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="running-header">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="marker"/>
			</xs:choice>
			<xs:attribute name="display" default="yes">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="no"/>
						<xs:enumeration value="yes"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="reintroduction-code">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="marker"/>
				<xs:element ref="added-phrase"/>
				<xs:element ref="deleted-phrase"/>
				<xs:element ref="internal-xref"/>
				<xs:element ref="external-xref"/>
				<xs:element ref="fraction"/>
				<xs:element ref="superscript"/>
				<xs:element ref="subscript"/>
				<xs:element ref="italic"/>
				<xs:element ref="quote"/>
				<xs:element ref="inline-comment"/>
				<xs:element ref="act-name"/>
				<xs:element ref="formula"/>
				<xs:element ref="term"/>
				<xs:element ref="omitted-text"/>
				<xs:element ref="linebreak"/>
				<xs:element ref="nobreak"/>
				<xs:element ref="pagebreak"/>
			</xs:choice>
			<xs:attribute name="display" default="yes">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="no"/>
						<xs:enumeration value="yes"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="suspension-floor-action">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="marker"/>
				<xs:element ref="added-phrase"/>
				<xs:element ref="deleted-phrase"/>
				<xs:element ref="internal-xref"/>
				<xs:element ref="external-xref"/>
				<xs:element ref="fraction"/>
				<xs:element ref="superscript"/>
				<xs:element ref="subscript"/>
				<xs:element ref="italic"/>
				<xs:element ref="quote"/>
				<xs:element ref="inline-comment"/>
				<xs:element ref="act-name"/>
				<xs:element ref="formula"/>
				<xs:element ref="term"/>
				<xs:element ref="omitted-text"/>
				<xs:element ref="linebreak"/>
				<xs:element ref="nobreak"/>
				<xs:element ref="pagebreak"/>
			</xs:choice>
			<xs:attribute name="display" default="yes">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="no"/>
						<xs:enumeration value="yes"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="suspension-instructions">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="marker"/>
				<xs:element ref="added-phrase"/>
				<xs:element ref="deleted-phrase"/>
				<xs:element ref="internal-xref"/>
				<xs:element ref="external-xref"/>
				<xs:element ref="fraction"/>
				<xs:element ref="superscript"/>
				<xs:element ref="subscript"/>
				<xs:element ref="italic"/>
				<xs:element ref="quote"/>
				<xs:element ref="inline-comment"/>
				<xs:element ref="act-name"/>
				<xs:element ref="formula"/>
				<xs:element ref="term"/>
				<xs:element ref="omitted-text"/>
				<xs:element ref="linebreak"/>
