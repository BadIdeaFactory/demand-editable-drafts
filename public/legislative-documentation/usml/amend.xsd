		</xs:complexType>
	</xs:element>
	<xs:element name="rule">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="enum"/>
				<xs:element ref="header"/>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="rules-clause"/>
					<xs:element ref="continuation-text"/>
					<xs:element ref="appropriations-major"/>
					<xs:element ref="appropriations-intermediate"/>
					<xs:element ref="appropriations-small"/>
					<xs:element ref="chapter"/>
					<xs:element ref="subdivision"/>
					<xs:element ref="division"/>
					<xs:element ref="subsection"/>
					<xs:element ref="paragraph"/>
					<xs:element ref="subparagraph"/>
					<xs:element ref="clause"/>
					<xs:element ref="subclause"/>
					<xs:element ref="item"/>
					<xs:element ref="subitem"/>
					<xs:element ref="part"/>
					<xs:element ref="section"/>
					<xs:element ref="subchapter"/>
					<xs:element ref="subpart"/>
					<xs:element ref="subtitle"/>
					<xs:element ref="title"/>
					<xs:element ref="legis-comment"/>
				</xs:choice>
			</xs:sequence>
			<xs:attribute name="id" use="required" type="xs:ID"/>
			<xs:attribute name="changed">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="added"/>
						<xs:enumeration value="deleted"/>
						<xs:enumeration value="not-changed"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
