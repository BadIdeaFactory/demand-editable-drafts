	<xs:element name="first-page-desc">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="marker"/>
			</xs:choice>
			<xs:attribute name="display" default="yes">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
